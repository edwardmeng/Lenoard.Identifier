{
  "version": "1.0.0-*",

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "System.Net.NameResolution": "4.0.0" 
  },

  "frameworks": {
    "netstandard1.3": {
      "imports": "dnxcore50",
      "buildOptions": {
        "compile": {
          "include": ["../../GlobalAssemblyInfo.cs", "../../src/Lenoard.Identifier/**/*.cs"]
        },
        "embed": {
          "include": ["../../src/*.resx"]
        },
        "define": ["NetCore"],
        "keyFile": "../../Lenoard.snk",
        "xmlDoc": true,
        "outputName": "Lenoard.Identifier"
      }
    }
  }
}
